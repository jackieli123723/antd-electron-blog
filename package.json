{
  "name": "West-Gate-Electron",
  "version": "0.1.0",
  "description": "A Eleactron app with react blog admin",
  "author": "jackieli",
  "main": "public/electron.js",
  "homepage": "./",
  "private": true,
  "build": {
    "appId": "com.demo"
  },
  "mac": {
    "category": "public.app-category.productivity",
    "target": "dmg"
  },
  "win": {
    "certificateFile": "private/stackacademy.pfx",
    "verifyUpdateCodeSignature": false,
    "publisherName": "stackacademy",
    "target": [
      {
        "target": "nsis",
        "arch": [
          "x64",
          "ia32"
        ]
      }
    ]
  },
  "linux": {
    "target": [
      "AppImage",
      "deb"
    ]
  },
  "devDependencies": {
    "cross-env": "^5.2.1",
    "electron": "^6.0.9",
    "electron-builder": "^21.2.0"
  },
  "dependencies": {
    "antd": "^3.22.0",
    "axios": "^0.19.0",
    "babel-plugin-import": "^1.12.0",
    "braft-editor": "^2.3.7",
    "customize-cra": "^0.5.0",
    "less": "^3.10.1",
    "less-loader": "^5.0.0",
    "moment": "^2.24.0",
    "qs": "^6.8.0",
    "react": "^16.9.0",
    "react-app-rewired": "^2.1.3",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.1"
  },
  "scripts": {
    "start-dev": "react-scripts start",
    "build-dev": "react-scripts build",
    "test-dev": "react-scripts test",
    "eject-dev": "react-scripts eject",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "start-electron": "cross-env NODE_ENV=development electron .",
    "packager": "npm run build && rimraf  dist && electron-builder",
    "mac": "npm run build && rimraf  dist && electron-builder -m",
    "linux": "npm run build && rimraf  dist && electron-builder -l",
    "win": "npm run build && rimraf  dist && electron-builder -w", 
    "packager:prod": "npm run build && rimraf dist && cross-env NODE_ENV=production electron-builder"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
